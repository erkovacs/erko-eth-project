{
  "name": "erko-eth-project",
  "version": "1.0.0",
  "description": "Etherium dApp project (decentralised double-blind study support app)",
  "main": "truffle-config.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd ./src/server/ && npm run start",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erkovacs/erko-eth-project.git"
  },
  "author": "erko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erkovacs/erko-eth-project/issues"
  },
  "homepage": "/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@openzeppelin/contracts": "3.3.0",
    "@truffle/hdwallet-provider": "1.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "bootstrap": "4.5.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bignumber": "3.0.0",
    "dotenv": "8.2.0",
    "react": "16.13.1",
    "react-bootstrap": "1.3.0",
    "react-datepicker": "^3.6.0",
    "react-dom": "16.13.1",
    "react-input-slider": "^6.0.0",
    "react-scripts": "3.4.3",
    "truffle": "5.1.63",
    "truffle-hdwallet-provider-privkey": "0.3.0",
    "web3": "1.2.6"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
